pipeline {
    agent any
   
    stages {

      stage('clean')
            {
                steps
                 { 
                    sh 'mvn clean package'
                 }
            }
        stage("build & SonarQube analysis") {
            steps {
              withSonarQubeEnv('sonarqube') {
                sh 'mvn clean package sonar:sonar'
              }
            } 
        }
       stage('deploy'){
        steps{
            
           sh 'scp  -o StrictHostKeyChecking=no target/*.war ec2-user@52.66.212.90:apache-tomcat-9.0.26/webapps/'
        }
       
       }
}
}
