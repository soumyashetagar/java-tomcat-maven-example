pipeline {
    agent any
   
    stages {

      stage('clean')
            {
                steps
                 { 
                    sh 'mvn clean package'
                 }
            }
        stage("build & SonarQube analysis") {
            steps {
              withSonarQubeEnv('sonarqube') {
                sh 'mvn clean package sonar:sonar'
              }
            } 
        }
       stage('deploy'){
        steps{
            
           sh 'cp  /var/lib/jenkins/workspace/DeployPipeline/target/java-tomcat-maven-example.war	 /var/lib/tomcat9/webapps/'
        }
       
       }
}
}
